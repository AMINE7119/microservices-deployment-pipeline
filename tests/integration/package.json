{
  "name": "integration-tests",
  "version": "1.0.0",
  "description": "Integration tests for microservices deployment pipeline",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": ["integration", "microservices", "testing"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000,
    "testMatch": [
      "**/*.integration.test.js"
    ]
  }
}